[user]
	name = Yuki Yoshida
	email = yuki.yoshida.dev@gmail.com
[core]
	editor = code --wait
	autocrlf = input
	excludesfile = ~/.gitignore_global
[init]
	defaultBranch = main
[pull]
	rebase = false
[push]
	autoSetupRemote = true
[alias]
	st = status
	co = checkout
	br = branch
	cm = commit
	lo = log --oneline -n 20
